generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model usuarios {
  id    String @id @default(uuid())
  nome  String
  email String @unique
  senha String
}

model produtos {
  id        String @id @default(uuid())
  nome      String
  descricao String
  preco     Float
  imagem    String
}